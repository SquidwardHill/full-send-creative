{
  "name": "vercel-remix-full-send",
  "private": true,
  "sideEffects": [
    "*.css"
  ],
  "type": "module",
  "prisma": {
    "seed": "npx tsx scripts/importCaseStudies.ts"
  },
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "typecheck": "tsc",
    "seed": "npx prisma generate && npx tsx scripts/importCaseStudies.ts",
    "migrate": "./scripts/build.sh",
    "clearCache": "rm -rf node_modules && rm -rf prisma/node_modules && npm i",
    "inspect": "npx prisma studio",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "dev:reset": "npx prisma generate && npx prisma migrate reset --force",
    "dev:css": "npx @tailwindcss/cli -i ./app/assets/styles/index.css -o ./app/assets/styles/compiled.css --watch",
    "dev:full": "concurrently \"npm run dev:css\" \"npm run dev\""
  },
  "dependencies": {
    "@iconify/react": "^6.0.0",
    "@remix-run/node": "2.10.0",
    "@remix-run/react": "2.10.0",
    "@remix-run/server-runtime": "2.10.0",
    "@tailwindcss/cli": "^4.1.7",
    "@tailwindcss/vite": "^4.1.7",
    "@vercel/analytics": "^1.2.2",
    "@vercel/remix": "2.10.0",
    "date-fns": "^4.1.0",
    "isbot": "^4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@prisma/client": "^6.8.2",
    "@remix-run/dev": "2.10.0",
    "@remix-run/eslint-config": "2.10.0",
    "@types/node": "^22.14.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prisma": "^6.7.0",
    "tailwindcss": "^4.1.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
